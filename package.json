{
  "name": "vue-material-you",
  "version": "0.0.5",
  "description": "Material design 3 UI framework for Vue 3",
  "scripts": {
    "dev:docs": "cross-env ENV=docs vite",
    "build:docs": "cross-env ENV=docs vite-ssg build",
    "serve:docs": "cross-env ENV=docs vite preview",
    "build": "cross-env ENV=prod vite build",
    "test": "cross-env ENV=test vitest run",
    "test:watch": "cross-env ENV=test vitest",
    "test:ui": "cross-env ENV=test vitest --ui",
    "test:coverage": "cross-env ENV=test vitest run --coverage",
    "test:watch:coverage": "cross-env ENV=test vitest --coverage",
    "lint": "eslint --ext .ts,.tsx,.vue --fix ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "typescript",
    "material",
    "design",
    "ui",
    "framework",
    "vue-framework"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "main": "./dist/vue-material-you.es.js",
  "module": "./dist/vue-material-you.es.js",
  "author": "Web-TS",
  "license": "MIT",
